<html>
	<head>
		<title>{N_:Login:}</title>
	</head>
	
	<body>
		<form action="/$appname/check" method="post">
			<table>
				<tr>
					<td><b>{N_:Username:}</b></td>
					<td><input type="text" name="username"/></td>
				</tr>
				<tr>
					<td><b>{N_:Password:}</b></td>
					<td><input type="password" name="password"/></td>
				</tr>
				<tr>
					<td colspan="2"><div align="right"><input type="Submit" value="{N_:Login:}"/></td>
				</tr>
			</table>
		</form>
	</body>
</html>

		